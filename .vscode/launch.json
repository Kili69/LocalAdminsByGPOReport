    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": []
        }
        {
            "name": "PowerShell: launch for dev domain",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": ["-domain 'dev.bloedgelaber.de'"]
        }
    ]
}